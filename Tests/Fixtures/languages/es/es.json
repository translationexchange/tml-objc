                         ],
            "token_mapping":[
                             "unsupported",
                             {
                             "one":"{$0}",
                             "other":"{$1}"
                             }
                             ],
            "rules":{
                "one":{
                    "keyword":"one",
                    "description":"{n} is 1",
                    "examples":"1",
                    "conditions":"(= @n 1)",
                    "conditions_expression":[
                                             "=",
                                             "@n",
                                             1
                                             ]
                },
                "other":{
                    "keyword":"other",
                    "examples":"0, 2-999; 1.2, 2.07..."
                }
            }
        }
    },
